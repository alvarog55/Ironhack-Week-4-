<h1> List of Users </h1>
<%= flash_message %>
<ul>
	<% @users.each do |user| %>
		<li>
			Username: <%= link_to(user.name, user_path(user)) %>
			<%= link_to "Delete User", user_path(user), method: :delete, data: {confirm: "Are you sure?"} %>
		</li>

	<% end %>
</ul>

<%= link_to "Create User", new_user_path %>